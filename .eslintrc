{
  "extends": "@antfu",
  "rules": {
    "no-restricted-syntax": "off",
    "curly": "off",
    "brace-style": "off",
    "no-cond-assign": "off",
    "react/display-name": "off",
    "react/no-unknown-property": "off",
    "vue/require-slots-as-functions": "off",
    "vue/return-in-computed-property": "off",
  }
}
